<div class="list-card-container">
    <div class="list-card">
        <div class="list-card-heading col-xs-12">
            <span class="list-card-title">{{item.title}}</span>
        </div>
        <div class="col-xs-12 list-card-image">
            <div class="cardImage" style="background-image: url('{{item.photo}}')"></div>
        </div>
        <div class="col-xs-12 list-card-info">
            <div>{{item.price}}</div>
        </div>
        <div class="col-xs-12 list-card-button">
            
        </div>
    </div>
</div>

